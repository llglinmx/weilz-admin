<template>
	<view class="box">
		<view class="box-head" :style="{paddingTop:barHeight+'px'}">
			<nav-title-balck :navTitle="title"></nav-title-balck>
		</view>
		<view class="box-content">
			<view class="box-content-list" :style="{display:isData?'block':'none'}">
				<view class="box-content-list-li" v-for="(item,index) in dataList" :key='index'
					:class="item.use_status==1?'bgColor':''">
					<view class="box-content-list-li-img">
						<image :src="item.simg" mode="aspectFill"></image>
					</view>
					<view class="box-content-list-li-info">
						<view class="box-content-list-li-info-item">
							<text class="iconfont iconshizhong icon-font" style="font-size: 28rpx;"></text>
							<text>{{item.plan_start}}-{{item.plan_end}}AM</text>
						</view>
						<view class="box-content-list-li-info-item">
							<text class="iconfont icon70BasicIcons-all-05 icon-font" style="font-size: 28rpx;"></text>
							<text>{{item.user_name}}</text>
						</view>
						<view class="box-content-list-li-info-item">
							<text class="iconfont icondingdan1 icon-font" style="font-size: 28rpx;"></text>
							<text>{{item.content}}</text>
						</view>
					</view>
					<view class="box-content-list-li-name">
						{{item.member_name}}
					</view>
					<view class="box-content-list-li-complete flex-center" v-if="item.use_status==1">
						<text class="iconfont iconxuanzhong2" style="font-size: 40rpx;"></text>
					</view>
				</view>
			</view>
			<view class="box-content-list" style="padding: 0;" :style="{display:!isData?'block':'none'}">
				<loading-merchant v-if="isLoad" />
				<no-data msg="暂无任何技师的排班日程" v-if="!isLoad" />
			</view>
		</view>
	</view>
</template>

<script>
	import {
		timeList
	} from '../../static/js/publicFile.js'

	export default {
		data() {
			return {
				barHeight: 0, //顶部电量导航栏高度
				title: '2021年02月12日预约日程',
				timeList: [],
				dataList: [],
				keyIndex: 0,
				dataArr: {},
				msg: '分钟',
				isLoad: true,
				isData: false,
			};
		},

		onLoad(options) {
			// 获取顶部电量状态栏高度
			uni.getSystemInfo({
				success: (res) => {
					this.barHeight = res.statusBarHeight
				}
			});
			var data = JSON.parse(options.data)
			this.dataArr = data
			this.title = data.year + '年' + data.month + '月' + data.day + "日预约日程"

			this.getSchedulingInfo(data)
		},
		methods: {

			getSchedulingInfo(e) {
				this.apiget('api/v1/store/engineer/engineer_schedule', {
					store: e.store,
					date: (e.year + '-' + e.month + '-' + e.day),
				}).then(res => {
					if (res.status == 200) {
						var list = res.data
						this.isLoad = true
						if (list.length != 0) {
							this.dataList = []
							res.data.map(item => {
								item.order_info.map(ele => {
									let start = this.timeChange(ele.plan_start)
									let end = this.timeChange(ele.plan_end)

									ele.simg = item.simg
									ele.content = ele.reserve_name + ' ' + (end - start) + this.msg
									this.dataList.push(ele)
								})
							})
						}
						if (this.dataList.length != 0) {
							this.isData = true
						}
						this.isLoad = false
					}
				});
			},

			timeChange(item) {
				var t = item.split(':');
				var times = 60 * parseInt(t[0]) + parseInt(t[1])
				return times
			},
		}
	}
</script>

