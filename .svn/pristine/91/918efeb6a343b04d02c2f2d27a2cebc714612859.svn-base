<script>
	import navTitleWhite from "../../components/nav-title-white/nav-title-white.vue"
	export default {
		data() {
			return {
				lan: {},
				barHeight: 0, //顶部电量导航栏高度
				id: '',
				orderInfo: {

				}
			};
		},
		components: {
			navTitleWhite
		},
		onReady() {
			// 获取顶部电量状态栏高度
			uni.getSystemInfo({
				success: (res) => {
					this.barHeight = res.statusBarHeight
				}
			});
		},
		onLoad(options) {
			this.id = options.id
			this.getOrderDetails(options.id)
			this.getLanguage()
		},
		methods: {

			// 取消订单
			cancelOrder() {
				this.$refs.popup.open()
			},

			confirm() {
				this.apiput('api/v1/store/order/cancel_order/' + this.id).then(res => {
					if (res.status == 200) {
						this.$store.commit('upOrderState', true)
						uni.showToast({
							title: this.lan.canceledSuccessfullyz,
							icon: "none"
						})
						setTimeout(() => {
							uni.navigateBack({
								delta: 1
							})
						}, 500)
					} else {
						uni.showToast({
							title: res.massage,
							icon: 'none'
						})
					}
				});
			},

			// 收到现金
			cashOrder() {
				this.$refs.popupCash.open()
			},
			cashConfirm() {
				this.apiput('api/v1/store/order/cash_received/'+this.orderInfo.id, {}).then(res => {
					if (res.status == 200) {
						this.$store.commit('upOrderState', true)
						uni.showToast({
							title: '订单确定成功',
							icon: 'none'
						})
						setTimeout(() => {
							uni.navigateBack({
								delta: 1
							})
						}, 500)
					} else {
						uni.showToast({
							title: res.massage,
							icon: 'none'
						})
					}
				});
			},
			
			// 提醒顾客
			remindCustomers() {
				this.apiget('api/v1/store/order/reminder_service', {
					order: this.orderInfo.id
				}).then(res => {
					if (res.status == 200) {
						uni.showToast({
							title: this.lan.sentSuccessfullyz, //"短信提醒已发送成功",
							icon: 'none'
						})
						this.getOrderDetails(this.id)
					} else {
						uni.showToast({
							title: res.massage,
							icon: 'none'
						})
					}
				});
			},
			// 已发送提醒
			hasBeenSent() {
				uni.showToast({
					title: this.lan.sendAgain, //"短信提醒已发送，请勿重复发送",
					icon: 'none'
				})
			},

			// 确认核销
			confirmWriteOff() {
				this.$refs.popupWriteOff.open()
			},
			confirm() {
				this.apiput('api/v1/store/order/write_off/' + this.id, {}).then(res => {
					if (res.status == 200) {

						this.$store.commit('upOrderState', true)
						uni.showToast({
							title: this.lan.verificationSucceededS,
							icon: "none"
						})
						setTimeout(() => {
							uni.navigateBack({
								delta: 1
							})
						}, 500)


					} else {
						uni.showToast({
							title: res.massage,
							icon: "none"
						})
					}
				});
			},
			// 获取订单详情 
			getOrderDetails(id) {
				this.apiget('api/v1/store/order/' + id).then(res => {
					if (res.status == 200) {
						this.orderInfo = res.data.data
					} else {
						uni.showToast({
							title: res.massage,
							icon: 'none'
						})
					}
				});
			},
			// 请求语言包
			getLanguage() {
				this.apiget('language/info', {
					name: 'MerchantOrder'
				}).then(res => {
					if (res.status == 200) {
						let language = res.data.language
						this.lan = res.data.language


					}
				});
			},
		}
	}
</script>

